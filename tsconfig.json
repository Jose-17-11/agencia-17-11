{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@common/*": ["src/components/common/*"],
      "@animations/*": ["src/components/animations/*"],
      "@sectionsMain/*": ["src/components/sectionsMain/*"],
      "@sectionsBlog/*": ["src/components/sectionsBlog/*"],
      "@layouts/*": ["src/layouts/*"],
      "@assets/*": ["src/assets/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react",
  },
  
}